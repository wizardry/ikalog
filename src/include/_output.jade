							select
					dl
						dt 登録時のウデマエを指定する
						dd
							select
					dl
						dt ユーザーを指定する
						dd
							select

	.viewArea.js-outputView
		dl
			dt k/d比
			dd.js-scoreRatio
		dl
			dt k/d平均
			dd.js-scoreAverage
		dl
			dt 勝率
			dd.js-winRatio
		script(type="text/html" id="outputListTemp")
			h3.viewTitle 
				span(class!="result <%= resultClass() %>") <%= _result() %>
				span.date <%= date %>
			table.viewTableRow
				tr.kill
					th kill
					td <%= kill %>
				tr.death
					th death
					td <%= death %>
				tr.rule
					th rule
					td <%= _rule() %>
				tr.stage
					th stage
					td <%= _stage() %>
				tr.comment
					th comment
					td <%= comment %>
			p.editButtonWrap
				a.editButton.js-edit(data-modelid!="<%= id%>") この戦績を編集する

		script(type="text/html" id="outputListWrapTemp")
			.sectionBlock
		div#outputListWrap
		div.hide.nullField
			p 登録データがありません。